<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cv-win" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin\Debug\cv" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin\Debug\" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Option parameters="tree.avi" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin\Release\cv" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
			<Add directory="$(#mbsutil.include)" />
			<Add directory="$(#opencv.include)\opencv" />
		</Compiler>
		<Linker>
			<Add library="mbsutil" />
			<Add library="libcv210.dll.a" />
			<Add library="libcvaux210.dll.a" />
			<Add library="libcxcore210.dll.a" />
			<Add library="libhighgui210.dll.a" />
			<Add directory="$(#opencv.lib)" />
			<Add directory="$(#mbsutil.lib)" />
		</Linker>
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<lib_finder disable_auto="1" />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
